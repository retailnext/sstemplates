<workbook xmlns="http://carbonfive.com/schema/sstemplates">
  <sheet name="StandAlone Example">
    <row>
      <cell>Name:</cell>
      <cell>${ stringValue }</cell>
    </row>
    <row>
      <cell>Names:</cell>
      <forEach var="name" items="${ listValue }">
        <cell>${ name }</cell>
      </forEach>
    </row>
  </sheet>
</workbook>