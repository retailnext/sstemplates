<workbook xmlns="http://carbonfive.com/schema/sstemplates">
  <sheet name="Accumulator Example">
    <row>

      <cell type="numeric" accumulator="sum">1</cell>
      <cell type="numeric" accumulator="sum">2</cell>
      <cell type="numeric" accumulator="sum">4</cell>
      <cell type="formula">sum(${accumulator.sum})</cell>
    </row>
  </sheet>
</workbook>